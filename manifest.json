{
  "manifest_version": 3,
  "name": "MettyNeo",
  "version": "2025.1.12,
  "description": "Adds a variety of UI features and improvements to Neopets",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "images/icon16.png",
      "48": "images/icon48.png",
      "128": "images/icon128.png"
    }
  }
  "content_scripts": [
    {
        "matches": ["*://*neopets.com*"],
        "js": ["mn_neo.js"]
    }
  ]
}